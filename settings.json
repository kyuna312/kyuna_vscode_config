{
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "eq-material-theme-icons-palenight",
    "workbench.colorTheme": "Vitesse qb dark pink",
    "workbench.productIconTheme": "IntelliJ",  
    "workbench.statusBar.visible": false,
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "update.showReleaseNotes": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDelete": false,
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.fontFamily": "Dank Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.lineHeight": 1.75,
    "editor.suggestFontSize": 0,
    "editor.suggestLineHeight": 28,
    "editor.formatOnSave": true,
    "editor.lightbulb.enabled": "off",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "boundary",
    "editor.overviewRulerBorder": false,
    "editor.scrollbar.verticalScrollbarSize": 4,
    "editor.scrollbar.horizontalScrollbarSize": 4,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.renderLineHighlight": "all",
    "editor.renderControlCharacters": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": true,
    "editor.minimap.enabled": false,
    "editor.glyphMargin": false,
    "editor.folding": true,
    "editor.lineNumbers": "on",
    "breadcrumbs.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": false,
    "extensions.ignoreRecommendations": true,
    "files.insertFinalNewline": true,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.env.windows": {},
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.fontFamily": "monospace",
    "scm.diffDecorations": "none",
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.fontSize": 14,
    "telemetry.telemetryLevel": "off",
    "workbench.colorCustomizations": {
        "terminalCursor.foreground": "#C45DFF",
        "terminal.background": "#00000000",
        "editorGroupHeader.tabsBackground": "#00000000",
        "sideBar.background": "#00000000",
        "tab.activeBackground": "#00000000",
        "statusBar.noFolderBackground": "#00000000"
    },
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.enablePreview": false,
    "zenMode.hideLineNumbers": true,
    "window.zoomLevel": 1,
    "window.nativeFullScreen": true,
    "window.titleBarStyle": "native",
    "editor.wordWrap": "on",
    "prettier.printWidth": 120,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.tsdk": "node_modules/typescript/lib",
    "eslint.validate": [
        "javascript", "typescript", "javascriptreact", "typescriptreact", "python", "html", "vue", "json", "jsonc", "ruby", 
        "shellscript", "yaml", "markdown", "css", "scss", "less", "java", "go", "lua", "c", "cpp", "rust"
    ],
    "cmake.configureOnOpen": true,
    "cmake.options.statusBarVisibility": "visible",
    "cmake.showOptionsMovedNotification": false,
    "python.defaultInterpreterPath": "/Users/maririn312/zephyrproject/.venv",
    "settingsSync.ignoredSettings": ["editor.tabSize"],
    "settingsSync.ignoredExtensions": ["github.copilot"],
    "[typescriptreact]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[python]": {
        "editor.defaultFormatter": "remimarsal.prettier-now"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "remimarsal.prettier-now"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[less]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[ruby]": {
        "editor.defaultFormatter": "rebornix.ruby"
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[html]": {
        "editor.defaultFormatter": "remimarsal.prettier-now"
    },
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[java]": {
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "[lua]": {
        "editor.defaultFormatter": "trixnz.vscode-lua"
    },
    "[c]": {
        "editor.wordBasedSuggestions": "off",
        "editor.semanticHighlighting.enabled": true
    },
    "debug.javascript.autoAttachFilter": "onlyWithFlag",
    "debug.node.autoAttach": "on",
    "debug.saveBeforeStart": "allEditorsInActiveGroup",
    "debug.console.fontSize": 14,
    "ruby.useBundler": true,
    "go.useLanguageServer": true,
    "javascript.debugger.usePreview": true,
    "rust-analyzer.checkOnSave.command": "clippy",
    "workbench.activityBar.location": "hidden"
}
